<%= form_for @feed do |f| %>
  <div class="clearfix">
    <%= f.label :name, 'Name' %>
    <div class="input">
      <%= f.text_field :name %>
    </div>
  </div>

  <div class="clearfix">
    <%= f.label :sms_identifier, 'Identifier' %>
    <div class="input">
      <%= f.text_field :sms_identifier %>
    </div>
  </div>

  <%= f.submit 'Save', :class => 'btn primary'%>
<% end %>
